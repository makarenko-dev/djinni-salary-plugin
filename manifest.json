{
  "name": "Djinni salary viewer",
  "description": "Finds hidden salary on Djinni",
  "version": "1.0",
  "manifest_version": 3,
  "action": { "default_popup": "popup.html" },
  "content_scripts": [ 
    {
        "matches":["https://djinni.co/*"],
        "js": ["scripts/content.js"]
    }
  ],
  "background": {
    "service_worker": "scripts/background.js",
    "type": "module"
  },
  "host_permissions": [
    "http://localhost:8000/*"
  ]
}


